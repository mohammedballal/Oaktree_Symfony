{% trans_default_domain 'FOSUserBundle' %}

{% form_theme form 'bootstrap_3_layout.html.twig' %}

<div class="p-a-md box-color r box-shadow-z1 text-color m-a">
    <form class="panel-body" data-validate="parsley" action="{{ path('fos_user_resetting_reset', {'token': token}) }}" {{ form_enctype(form) }} method="POST" class="fos_user_resetting_reset">
        {{ form_widget(form) }}
        <div>
            <input class="btn primary btn-block p-x-md" type="submit" value="{{ 'resetting.reset.submit'|trans }}" />
        </div>


    </form>
</div>

<div class="">
    <a href="{{ path('fos_user_security_login') }}"><i class="fa fa-angle-double-left " style="margin-top: 10px;"> Go Back</i></a>
</div>