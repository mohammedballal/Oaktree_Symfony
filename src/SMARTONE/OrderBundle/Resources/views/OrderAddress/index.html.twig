{% extends '::base.html.twig' %}

{% block body -%}
<div class="padding">
    <div class="row">
        <div class="col-md-12">
            <div class="box">

                <div class="box-header">
                    <h2>Orderaddress list</h2>
                </div>

    <table class="table table-striped table-condensed">
        <thead>
            <tr>
                <th>Id</th>
                <th>Address</th>
                <th class="text-right">Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('orderaddress_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.address|nl2br }}</td>
                <td align="right">
                    <a class="btn btn-xs btn-info" href="{{ path('orderaddress_edit', { 'id': entity.id }) }}">Edit</a>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
                    <div class="dker p-a text-right">
                    <a ui-sref="app.widget" class="btn info" href="{{ path('orderaddress_new') }}">
                        New Orderaddress
                    </a>
                </div>
                </div>

        </div>
    </div>
</div>

{% endblock %}
