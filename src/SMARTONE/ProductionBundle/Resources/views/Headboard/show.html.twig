{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Headboard</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Headboardstyle</th>
                <td>{{ entity.headboardStyle }}</td>
            </tr>
            <tr>
                <th>Width</th>
                <td>{{ entity.width }}</td>
            </tr>
            <tr>
                <th>Colour</th>
                <td>{{ entity.colour }}</td>
            </tr>
            <tr>
                <th>Prefixcomment</th>
                <td>{{ entity.prefixComment }}</td>
            </tr>
            <tr>
                <th>Sortorder</th>
                <td>{{ entity.sortOrder }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('headboard') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('headboard_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
