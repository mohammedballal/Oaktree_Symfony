{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Feet</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Feetname</th>
                <td>{{ entity.feetName }}</td>
            </tr>
            <tr>
                <th>Feetsize</th>
                <td>{{ entity.feetSize }}</td>
            </tr>
            <tr>
                <th>Feetcolour</th>
                <td>{{ entity.feetColour }}</td>
            </tr>
            <tr>
                <th>Feettype</th>
                <td>{{ entity.feetType }}</td>
            </tr>
            <tr>
                <th>Sortorder</th>
                <td>{{ entity.sortOrder }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('feet') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('feet_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
